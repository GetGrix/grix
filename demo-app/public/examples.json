{
  "examples": [
    {
      "id": "basic-slope",
      "title": "Basic Slope Visualization",
      "description": "Explore how rise over run creates slope",
      "difficulty": "beginner",
      "category": "slopes",
      "objects": [
        {
          "type": "ray",
          "properties": {
            "startPoint": { "x": 0, "y": 0 },
            "endPoint": { "x": 4, "y": 3 }
          }
        }
      ],
      "settings": {
        "showRiseRunTriangle": true,
        "showEquivalentFractions": true,
        "showDivisionAnswer": true,
        "showReferenceLineY_equals_X": true
      },
      "viewport": {
        "center": { "x": 2, "y": 1.5 },
        "zoom": 30
      }
    },
